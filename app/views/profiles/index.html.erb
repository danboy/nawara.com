<h1>Listing profiles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Description</th>
    <th>Birthday</th>
    <th>Phone number</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%=h profile.user_id %></td>
    <td><%=h profile.description %></td>
    <td><%=h profile.birthday %></td>
    <td><%=h profile.phone_number %></td>
    <td><%=h profile.address1 %></td>
    <td><%=h profile.address2 %></td>
    <td><%=h profile.city %></td>
    <td><%=h profile.state %></td>
    <td><%=h profile.zip %></td>
    <td><%= link_to 'Show', profile %></td>
    <td></td>
    <td></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profile', new_profile_path %>
